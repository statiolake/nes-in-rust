{
  "name": "Debian Trixie",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "workspaceFolder": "/workspace",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
  "remoteUser": "root",
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": ["ms-vscode.cpptools", "rust-lang.rust-analyzer"]
    }
  }
}
